<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\EmployeeType" table="employee_type">
    <id name="id" association-key="true"/>
    <field name="item" type="string" column="item" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="id" target-entity="Employee" fetch="LAZY">
      <join-columns>
        <join-column name="id" referenced-column-name="employee_type_id"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
