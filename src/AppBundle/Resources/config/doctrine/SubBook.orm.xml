<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\SubBook" table="sub_book">
    <indexes>
      <index name="sub_book_type_id" columns="sub_book_type_id"/>
      <index name="book_id" columns="book_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="item" type="string" column="item" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="false"/>
    <field name="updatedBy" type="integer" column="updated_by" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createdBy" type="integer" column="created_by" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="false"/>
    <many-to-one field="book" target-entity="Book" fetch="LAZY">
      <join-columns>
        <join-column name="book_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="subBookType" target-entity="SubBookType" fetch="LAZY">
      <join-columns>
        <join-column name="sub_book_type_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
